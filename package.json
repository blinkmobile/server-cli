{
  "name": "@blinkmobile/server-cli",
  "description": "CLI to develop, test and deploy server-side HTTPS endpoints with BlinkMobile",
  "version": "1.0.0",
  "bin": {
    "blinkm-server": "bin/index.js"
  },
  "bugs": {
    "url": "https://github.com/blinkmobile/server-cli/issues"
  },
  "dependencies": {
    "@blinkmobile/blinkmrc": "1.1.0",
    "@blinkmobile/bm-identity": "2.3.4",
    "@jokeyrhyme/pify-fs": "1.0.1",
    "archiver": "1.3.0",
    "aws-sdk": "2.43.0",
    "boom": "4.3.1",
    "chalk": "1.1.3",
    "cli-table2": "0.2.0",
    "cpr": "2.0.2",
    "elegant-spinner": "1.0.1",
    "execa": "0.6.3",
    "glob": "7.1.1",
    "good": "7.1.0",
    "good-console": "6.4.0",
    "hapi": "16.1.1",
    "inquirer": "3.0.6",
    "js-yaml": "3.8.3",
    "load-json-file": "2.0.0",
    "log-symbols": "^1.0.2",
    "log-update": "2.0.0",
    "meow": "3.7.0",
    "object-merge": "2.5.1",
    "pify": "2.3.0",
    "request": "2.81.0",
    "serverless": "1.9.0",
    "temp": "0.8.3",
    "uniloc": "0.3.0",
    "update-notifier": "2.1.0",
    "valid-url": "1.0.9",
    "write-json-file": "2.0.0"
  },
  "devDependencies": {
    "ava": "^0.19.0",
    "browserify": "^14.0.0",
    "eslint": "^3.11.0",
    "eslint-config-standard": "^10.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.0.0",
    "eslint-plugin-promise": "^3.3.2",
    "eslint-plugin-standard": "^3.0.0",
    "fixpack": "^2.3.1",
    "flow-bin": "^0.44.0",
    "npm-bin-ava-tester": "^1.1.0",
    "nyc": "^10.1.0",
    "proxyquire": "^1.7.10",
    "remark-cli": "^3.0.0",
    "remark-lint": "^6.0.0",
    "remark-preset-lint-recommended": "^2.0.0",
    "yauzl": "^2.7.0"
  },
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "test"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=3"
  },
  "files": [
    "bin",
    "dist",
    "commands",
    "lib",
    "scripts"
  ],
  "homepage": "https://github.com/blinkmobile/server-cli#readme",
  "keywords": [],
  "license": "MIT",
  "main": "index.js",
  "nyc": {
    "exclude": [
      "dist",
      "examples",
      "test"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blinkmobile/server-cli.git"
  },
  "scripts": {
    "ava": "nyc ava",
    "browserify": "browserify --node --no-insert-globals --no-detect-globals --standalone wrapper ./scripts/wrapper.js > ./dist/wrapper.js",
    "build": "npm run browserify",
    "eslint": "eslint --fix --cache .",
    "fixpack": "fixpack",
    "flow_check": "flow check",
    "lint-md": "remark . --use remark-preset-lint-recommended",
    "nyc": "nyc check-coverage",
    "posttest": "npm run eslint && npm run flow_check && npm run fixpack && npm run lint-md",
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "test": "npm run ava"
  }
}
